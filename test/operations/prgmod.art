fn @get_accelerator () {
    nvvm_accelerator(0)
}

fn @accelerated_matmul (nvvm : Accelerator, a : Tensor[f16], b : Tensor[f16], c : Tensor[f16], d : Tensor[f16]) -> () {
    if (M % 128 == 0 && N % 128 == 0) {
        matrix_multiply_nvvm_prgmod(nvvm, a, b, c, d);
    } else {
        matrix_multiply_nvvm(nvvm, a, b, c, d);
    }
}

fn @ref_matmul (a : Tensor[f16], b : Tensor[f16], c : Tensor[f16], d : Tensor[f16]) -> () {
    for x in range(0, c.x_dim) {
        for y in range(0, c.y_dim) {
            d.data(addr_tensor(x, y, d)) = c.data(addr_tensor(x, y, d));
        }
    }

    matrix_multiply_blas(a, b, d);
}
