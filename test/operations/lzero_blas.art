fn @get_accelerator () {
    levelzero_accelerator(0)
}

fn @accelerated_matmul (lzero : Accelerator, a : Tensor, b : Tensor, c : Tensor, d : Tensor) -> () {
    matrix_multiply_acc_simple [spv_mat_a_datatype, spv_mat_b_datatype, spv_acc_datatype] (lzero, a, b, c, d);
}

fn @ref_matmul (a : Tensor, b : Tensor, c : Tensor, d : Tensor) -> () {
    for x in range(0, c.x_dim) {
        for y in range(0, c.y_dim) {
            d.data(addr_tensor(x, y, d)) = c.data(addr_tensor(x, y, d));
        }
    }

    matrix_multiply_blas(a, b, d);
}
